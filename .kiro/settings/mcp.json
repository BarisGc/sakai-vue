{
  "mcpServers": {
    "fetch": {
      "command": "uvx",
      "args": ["mcp-server-fetch"],
      "env": {},
      "disabled": true,
      "autoApprove": []
    },
    "context7": {
      "autoApprove": [
        "get-library-docs",
        "resolve-library-id"
      ],
      "timeout": 60,
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {
        "DEFAULT_MINIMUM_TOKENS": ""
      }
    },
    "chrome-devtools": {
      "autoApprove": [
        "drag",
        "close_page",
        "click",
        "emulate_cpu",
        "emulate_network",
        "evaluate_script",
        "fill",
        "fill_form",
        "get_network_request",
        "handle_dialog",
        "hover",
        "list_console_messages",
        "list_network_requests",
        "list_pages",
        "navigate_page",
        "navigate_page_history",
        "new_page",
        "performance_analyze_insight",
        "performance_start_trace",
        "performance_stop_trace",
        "resize_page",
        "select_page",
        "take_screenshot",
        "take_snapshot",
        "upload_file",
        "wait_for"
      ],
      "disabled": true,
      "timeout": 60,
      "type": "stdio",
      "command": "npx",
      "args": [
        "chrome-devtools-mcp@latest"
      ]
    }
  }
}
